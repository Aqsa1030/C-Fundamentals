#include <iostream>
using namespace std;

int main() {
    int a = 5;
    int b;

    cout << "Enter a number: ";
    cin >> b;

    int sum = a + b;
    cout << "Sum = " << sum << endl;
    if (sum == 10) {
        cout << "Sum is 10" << endl;
    } else if (sum > 10) {
        cout << "Sum is greater than 10" << endl;
    } else {
        if (sum == 0) {
            cout << "Sum is zero" << endl;
        }
    }

    return 0;
}
