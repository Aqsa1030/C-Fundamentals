#include <iostream>
using namespace std;

int main() {
    int a , b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    if (a > b) {
        cout << "a is greater than b" << endl;
     }

    else if (a < b) {
        cout << "a is less than b" << endl; 
    }
    else {
        cout << "a is equal to b" << endl;
    }

    cout << endl;
    for (int i = 0; i < 10; i++) {
        cout << "Count: " << i << endl;
    }
    cout << endl;
    int arr[5];
    for (int i = 0; i < 5; i++) {
        arr[i] = i;
    }
    cout << endl;
    cout << "Enter your age: ";
    int age;
    cin >> age;
    if (age > 18) {
        cout << "You are an adult." << endl;
    }
        cout << "Welcome!" << endl; // Error 11: Always executed due to missing curly braces

    char grade;
    cout << "Enter your grade (A, B, C, D, F): ";
    cin >> grade;
    switch (grade) {
        case 'A':
        case 'B':
        case 'C':
            cout << "Pass" << endl;
        case 'D':
        case 'F':
            cout << "Fail" << endl; // Error 12: Missing break statements
    }

    return 0;
}

